      <div class="jumbotron" style="padding-top:5px;border-radius: 25px;">
        <center>
          <%= yield %>
        </center>
      </div>
    </div>
  </body>

  <p class="link">
    <b>
      <a class="one" href="/error" target="_blank">Whoa!</a>
    </b>
  </p>

  <p align="center" style="margin-bottom:40px;color:white;font-size:10px">Â© 2015 All Rights Reserved.</p>

  <%= render 'layouts/footer' %>

